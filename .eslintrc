{
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "globals": {
    "fetch": true,
    "navigator": true,
    "document": true,
    "window": true,
    "If": true,
    "Else": true,
    "For": true,
    "localStorage": true,
    "sessionStorage": true,
    "Headers": true,
    "btoa": true,
    "global": true,
    "FormData": true
  },
  "rules": {
    "class-methods-use-this": 0,
    "comma-dangle": 2,
    "global-require": 0,
    "import/prefer-default-export": 0,
    "object-curly-spacing": [2, "always"],
    "react/prefer-stateless-function": 0,
    "space-infix-ops": 2,
    "semi": [2, "never"],
    "no-empty": [2, {
      "allowEmptyCatch": true
    }],
    "consistent-return": 0,
    "no-param-reassign": 0,
    "no-return-assign": 0,
    "no-nested-ternary": 0
  }
}
